{
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "no-console": "off",
        "import/extensions": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn"
    },
    "extends": [
        "airbnb/base",
        "plugin:import/errors",
        "plugin:import/warnings"
    ]
}