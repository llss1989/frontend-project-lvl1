{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "space-in-parens": "warn",
        "no-console": "off",
        "import/extensions": "warn",
        "no-plusplus": "off",
        "eol-last": "off"
    },
    "extends": [
        "airbnb/base",
        "plugin:import/errors",
        "plugin:import/warnings"
    ]
}